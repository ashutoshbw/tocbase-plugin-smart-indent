{
  "type": "module",
  "name": "tocbase-plugin-smart-indent",
  "version": "2.0.0",
  "description": "A tocbase plugin for smartly adding indentation to numbered ToC items.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ashutoshbw/tocbase-plugin-smart-indent.git"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist/*",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "smart indentation",
    "smart-indent",
    "plugin",
    "tocbase",
    "table of contents",
    "toc"
  ],
  "author": {
    "name": "Ashutosh Biswas",
    "email": "ashutoshbw314@gmail.com",
    "url": "https://ashutoshbiswas.dev"
  },
  "license": "MIT",
  "bugs": "https://github.com/ashutoshbw/tocbase-plugin-smart-indent/issues",
  "devDependencies": {
    "rollup": "^2.78.0",
    "rollup-plugin-terser": "^7.0.2"
  }
}
